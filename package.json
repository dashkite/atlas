{
  "name": "@dashkite/atlas",
  "version": "0.6.1",
  "description": "Generate import maps",
  "keywords": [
    "import maps"
  ],
  "homepage": "https://github.com/dashkite/atlas#readme",
  "repository": "github:dashkite/atlas",
  "license": "See license in LICENSE.md",
  "author": "Dan Yoder",
  "exports": {
    ".": {
      "node": "./build/node/src/index.js"
    },
    "./generators": {
      "node": "./build/node/src/generators/index.js"
    },
    "./generators/*.js": {
      "node": "./build/node/src/generators/*.js"
    },
    "./presets/*": {
      "node": "./build/node/src/presets/*.js"
    }
  },
  "imports": {
    "#helpers/*": {
      "node": "./build/node/src/helpers/*.js"
    },
    "#generators": {
      "node": "./build/node/src/generators/index.js"
    },
    "#generators/*": {
      "node": "./build/node/src/generators/*.js"
    }
  },
  "files": [
    "build/node/src"
  ],
  "scripts": {
    "build": "genie build",
    "test": "genie test"
  },
  "dependencies": {
    "@dashkite/drn-sky": "link:../drn-sky",
    "@dashkite/joy": "^0.3.16",
    "@dashkite/polaris": "link:../polaris",
    "@dashkite/zephyr": "link:../zephyr",
    "esbuild": "^0.19.2",
    "js-yaml": "^4.1.0",
    "micromatch": "^4.0.5"
  },
  "devDependencies": {
    "@dashkite/amen": "^0.2.2",
    "@dashkite/amen-console": "^0.0.2",
    "@dashkite/assert": "^0.1.4",
    "@dashkite/genie": "link:../genie",
    "@dashkite/genie-clean": "link:../genie-clean",
    "@dashkite/genie-coffee": "link:../genie-coffee"
  }
}
